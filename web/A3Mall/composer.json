        "alibabacloud/client": "^1.5",
        "phpmailer/phpmailer": "~6.5",
        "liliuwei/thinkphp-jump": "^1.5",
        "topthink/think-worker": "^3.0",
        "phpoffice/phpspreadsheet": "^1.15",
